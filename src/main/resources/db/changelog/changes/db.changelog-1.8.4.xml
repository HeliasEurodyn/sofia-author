<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.10.xsd">

    <changeSet author="Helias" id="html_dashboard_script">
        <createTable tableName="html_dashboard_script">
            <column name="id" type="VARCHAR(36)">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="name" type="VARCHAR(255)"/>
            <column name="script" type="text"/>
            <column name="short_order" type="bigint(20)"/>
            <column name="html_dashboard_id" type="VARCHAR(36)">
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>
